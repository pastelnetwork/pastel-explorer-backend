{
  "name": "pastel-explorer-backend",
  "version": "1.1.0",
  "main": "index.js",
  "description": "Blockchain explorer for Pastel Network - compatible with other Bitcoin/Zcash like cryptocurrencies",
  "repository": "https://github.com/pastelnetwork/pastel-explorer-backend",
  "author": "Pastel Network",
  "license": "MIT",
  "scripts": {
    "start": "nodemon --files src/index.ts",
    "build": "tsc --project ./",
    "preinstall": "node -e \"if (process.env.npm_execpath.indexOf('yarn') === -1) throw new Error('Use yarn for installing')\"",
    "prepare": "husky install",
    "typeorm": "ts-node --files ./node_modules/typeorm/cli.js",
    "db:migrate": "yarn typeorm migration:run -- -d ./src/datasourcecon.ts",
    "lint": "eslint \"{src,}/**/*.ts\"",
    "type-check": "tsc --noEmit",
    "seedblockchain": "ts-node --files ./src/scripts/seed-blockchain-data/main.ts",
    "update-blocks": "ts-node --files ./src/scripts/script-update-block.ts",
    "update-senses": "ts-node --files ./src/scripts/script-update-senses.ts",
    "update-nfts": "ts-node --files ./src/scripts/script-update-nft.ts",
    "update-cascades": "ts-node --files ./src/scripts/script-update-cascade.ts",
    "update-tickets": "ts-node --files ./src/scripts/script-update-tickets.ts",
    "update-addresses": "ts-node --files ./src/scripts/script-update-address.ts",
    "update-all": "ts-node --files ./src/scripts/script-update-all.ts",
    "clean-tickets": "ts-node --files ./src/scripts/script-clean-tickets.ts",
    "update-coinsupply": "ts-node --files ./src/scripts/script-update-coinsupply.ts",
    "update-historical-market": "ts-node --files ./src/scripts/script-update-historical-market.ts"
  },
  "dependencies": {
    "@highlight-run/node": "3.7.3",
    "@opentelemetry/api": "1.7.0",
    "@opentelemetry/sdk-node": "0.48.0",
    "@opentelemetry/sdk-trace-base": "1.21.0",
    "@socket.io/redis-adapter": "8.2.1",
    "axios": "1.6.7",
    "better-sqlite3": "9.4.1",
    "bitcoin-core": "4.1.0",
    "coingecko-api": "1.0.10",
    "cors": "2.8.5",
    "cron": "3.1.6",
    "csv-writer": "1.6.0",
    "dayjs": "1.11.10",
    "dotenv": "16.4.4",
    "express": "4.19.2",
    "google-auth-library": "9.6.3",
    "highlight.run": "8.4.1",
    "js-base64": "3.7.6",
    "nodemailer": "6.9.9",
    "pm2": "5.3.1",
    "prompt": "1.3.0",
    "puppeteer": "22.1.0",
    "redis": "4.6.13",
    "reflect-metadata": "0.2.1",
    "slugify": "1.6.6",
    "socket.io": "4.7.4",
    "swagger-jsdoc": "6.2.8",
    "swagger-ui-express": "5.0.0",
    "typeorm": "0.3.20",
    "util": "0.12.5",
    "yup": "1.3.3"
  },
  "devDependencies": {
    "@types/express": "4.17.21",
    "@types/node": "20.11.19",
    "@types/nodemailer": "6.4.14",
    "@types/prompt": "1.1.8",
    "@types/socket.io": "3.0.2",
    "@types/swagger-jsdoc": "6.0.4",
    "@types/swagger-ui-express": "4.1.6",
    "@typescript-eslint/eslint-plugin": "7.0.1",
    "@typescript-eslint/parser": "7.0.1",
    "eslint": "8.56.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-simple-import-sort": "12.0.0",
    "husky": "9.0.11",
    "lint-staged": "15.2.2",
    "nodemon": "3.0.3",
    "prettier": "3.2.5",
    "ts-node": "10.9.2",
    "typescript": "5.3.3"
  }
}
