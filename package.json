{
  "name": "pastel-explorer-backend",
  "version": "1.1.0",
  "main": "index.js",
  "description": "Blockchain explorer for Pastel Network - compatible with other Bitcoin/Zcash like cryptocurrencies",
  "repository": "https://github.com/pastelnetwork/pastel-explorer-backend",
  "author": "Pastel Network",
  "license": "MIT",
  "scripts": {
    "start": "nodemon src/index.ts",
    "build": "tsc --project ./",
    "preinstall": "node -e \"if (process.env.npm_execpath.indexOf('yarn') === -1) throw new Error('Use yarn for installing')\"",
    "prepare": "husky install",
    "typeorm": "ts-node ./node_modules/typeorm/cli.js",
    "db:migrate": "npx ts-node ./node_modules/typeorm/cli.js migration:run",
    "lint": "eslint \"{src,}/**/*.ts\"",
    "type-check": "tsc --noEmit",
    "seedblockchain": "ts-node ./src/scripts/seed-blockchain-data/main.ts",
    "update-blocks": "ts-node ./src/scripts/script-update-block.ts",
    "update-senses": "ts-node ./src/scripts/script-update-senses.ts",
    "update-nfts": "ts-node ./src/scripts/script-update-nft.ts",
    "update-cascades": "ts-node ./src/scripts/script-update-cascade.ts",
    "update-tickets": "ts-node ./src/scripts/script-update-tickets.ts",
    "update-addresses": "ts-node ./src/scripts/script-update-address.ts",
    "update-all": "ts-node ./src/scripts/script-update-all.ts",
    "clean-tickets": "ts-node ./src/scripts/script-clean-tickets.ts",
    "update-historical-market": "ts-node ./src/scripts/script-update-historical-market.ts"
  },
  "dependencies": {
    "@highlight-run/node": "3.1.2",
    "axios": "^0.21.2",
    "better-sqlite3": "^7.4.5",
    "bitcoin-core": "^3.0.0",
    "coingecko-api": "^1.0.10",
    "cors": "^2.8.5",
    "cron": "^1.8.2",
    "csv-writer": "1.6.0",
    "dayjs": "^1.11.4",
    "dotenv": "^8.2.0",
    "express": "^4.17.3",
    "google-auth-library": "^8.7.0",
    "js-base64": "^3.7.3",
    "nodemailer": "^6.8.0",
    "pm2": "^5.2.0",
    "prompt": "^1.3.0",
    "puppeteer": "^10.2.0",
    "redis": "^3.1.2",
    "reflect-metadata": "^0.1.10",
    "slugify": "^1.6.6",
    "socket.io": "^4.1.3",
    "socket.io-redis": "^6.1.1",
    "swagger-jsdoc": "6.2.8",
    "swagger-ui-express": "4.6.2",
    "typeorm": "^0.2.32",
    "util": "0.12.4",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@types/express": "^4.17.11",
    "@types/node": "14.17.1",
    "@types/nodemailer": "^6.4.6",
    "@types/prompt": "^1.1.5",
    "@types/redis": "^2.8.31",
    "@types/socket.io": "^3.0.2",
    "@types/swagger-jsdoc": "6.0.1",
    "@types/swagger-ui-express": "4.1.3",
    "@typescript-eslint/eslint-plugin": "4.0.1",
    "@typescript-eslint/parser": "4.20.0",
    "eslint": "^7.24.0",
    "eslint-plugin-import": "2.20.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "husky": "^6.0.0",
    "lint-staged": "^10.5.4",
    "nodemon": "^2.0.7",
    "prettier": "^2.3.2",
    "ts-node": "3.3.0",
    "typescript": "^4.2.4"
  }
}
