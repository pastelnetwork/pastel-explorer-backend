{
  "name": "pastel-explorer-backend",
  "version": "1.0.0",
  "main": "index.js",
  "description": "Blockchain explorer for Pastel Network - compatible with other Bitcoin/Zcash like cryptocurrencies",
  "repository": "https://github.com/pastelnetwork/pastel-explorer-backend",
  "author": "Pastel",
  "contributors": [
    "Vatras (Intent) <fabisiak.tomek@gmail.com>"
  ],
  "license": "MIT",
  "scripts": {
    "start": "nodemon src/index.ts",
    "build": "tsc --project ./",
    "start:prod": "NODE_ENV=production node_modules/ts-node/dist/bin.js dist/index.js",
    "migration:run": "npx ts-node ./node_modules/typeorm/cli.js migration:run",
    "typeorm": "ts-node ./node_modules/typeorm/cli.js",
    "seedblockchain": "ts-node ./src/scripts/seed-blockchain-data/main.ts",
    "lint": "eslint \"{src,}/**/*.ts\"",
    "lint:fix": "eslint \"{src,}/**/*.ts\" --fix"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "bitcoin-core": "^3.0.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "http-proxy-middleware": "^1.1.1",
    "reflect-metadata": "^0.1.10",
    "sqlite3": "^5.0.2",
    "typeorm": "^0.2.32"
  },
  "devDependencies": {
    "@types/express": "^4.17.11",
    "@types/node": "^8.0.29",
    "@typescript-eslint/eslint-plugin": "4.0.1",
    "@typescript-eslint/parser": "4.20.0",
    "eslint": "^7.24.0",
    "eslint-plugin-import": "2.20.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "nodemon": "^2.0.7",
    "ts-node": "3.3.0",
    "typescript": "^4.2.4"
  }
}
